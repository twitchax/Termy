{
    "entrypoint": "index.html",
    "shell": "buildts/my-app.js",
    "sources": [
        "buildts/**/*",
        "package.json"
    ],
    "extraDependencies": [
        "node_modules/@webcomponents/webcomponentsjs/**/*",
        "node_modules/c3/**/*",
        "node_modules/d3/**/*"
    ],
    "builds": [
        {
            "name": "default",
            "preset": "es6-unbundled",
            "addServiceWorker": true,
            "addPushManifest": true
        }
    ],
    "moduleResolution": "node",
    "npm": true
}  